{
    "type": "module",
    "name": "@hinkage/pie3d_svg",
    "version": "2.0.8",
    "description": "3D pie chart supports variable heights of diffrent slices and inner radius with transition and auto rotating implemented by SVG.",
    "author": "hinkage",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/hinkage/pie3d_svg.git"
    },
    "keywords": [
        "pie",
        "pie chart",
        "pie 3d",
        "3d pie",
        "donut",
        "variable height"
    ],
    "publishConfig": {
        "access": "public"
    },
    "directories": {
        "example": "example",
        "lib": "lib"
    },
    "main": "src/pie3d.js",
    "scripts": {
        "dev": "rollup -c -w",
        "test": "jest --watch",
        "bs": "browser-sync start --server . --files \"example\" \"dist\" --startPath \"example/test.html\""
    },
    "prettier": {
        "trailingComma": "none",
        "useTabs": false,
        "tabWidth": 4,
        "semi": true,
        "singleQuote": true,
        "printWidth": 80
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^23.0.2",
        "@rollup/plugin-node-resolve": "^15.0.1",
        "@rollup/plugin-sucrase": "^5.0.1",
        "mockjs": "^1.1.0",
        "rollup-plugin-cleanup": "^3.2.1",
        "rollup-plugin-postcss": "^4.0.2",
        "tslib": "^2.4.1"
    },
    "dependencies": {
        "d3": "^7.6.1"
    }
}
